{
  "name": "boilerplate-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "generate:atom": "yarn plop --plopfile ./generators/plopfile.js atoms",
    "generate:mole": "yarn plop --plopfile ./generators/plopfile.js molecules",
    "generate:orgs": "yarn plop --plopfile ./generators/plopfile.js organisms",
    "postinstall": "husky install",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@styled-icons/bootstrap": "^10.34.0",
    "axios": "^0.24.0",
    "datocms-client": "^3.5.9",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1",
    "hamburger-react": "^2.4.1",
    "leaflet": "^1.7.1",
    "next": "11.0.1",
    "next-pwa": "^5.2.24",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "^3.2.2",
    "react-responsive": "^9.0.0-beta.4",
    "react-scroll": "^1.8.4",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.14.5",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/leaflet": "^1.7.5",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.14",
    "@types/react-scroll": "^1.8.3",
    "@types/react-slick": "^0.23.5",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/node": "^1.12.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.0.1",
    "plop": "^2.7.4",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  }
}
